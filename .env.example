# SwimVision Pro - Environment Configuration
# Copy this file to .env and update with your settings

# ========================================
# Database Configuration
# ========================================
POSTGRES_USER=swimvision
POSTGRES_PASSWORD=swimvision_dev
POSTGRES_DB=swimvision
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# ========================================
# Streamlit Configuration
# ========================================
STREAMLIT_PORT=8501

# ========================================
# pgAdmin Configuration (Optional)
# ========================================
PGADMIN_EMAIL=admin@swimvision.local
PGADMIN_PASSWORD=admin
PGADMIN_PORT=5050

# ========================================
# Model Configuration
# ========================================
YOLO_MODEL=yolo11n-pose.pt
YOLO_DEVICE=cpu
YOLO_CONFIDENCE=0.5

# ========================================
# Application Settings
# ========================================
DEBUG=False
LOG_LEVEL=INFO

# ========================================
# Camera Settings
# ========================================
DEFAULT_CAMERA_ID=0
DEFAULT_FPS=30
DEFAULT_RESOLUTION=1280x720
